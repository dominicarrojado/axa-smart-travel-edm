{
  "name": "axa-smart-travel-edm",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "start": "mjml --watch index.mjml -o index.html",
    "server": "live-server --host=localhost",
    "dev": "concurrently \"yarn start\" \"yarn server\"",
    "build": "mjml index.mjml -o index.html",
    "predeploy": "yarn build",
    "deploy": "gh-pages -d ."
  },
  "devDependencies": {
    "concurrently": "^6.2.0",
    "gh-pages": "^3.2.3",
    "live-server": "^1.2.1",
    "mjml": "^4.10.1"
  }
}
